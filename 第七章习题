#include<stdio.h>
int main(void) {
	char ch;
	while((ch = getchar()) != '#') {
		if(ch != '\n' && ch != 'c') {
			if(ch == 'b')
				break;
			if(ch == 'h') 
				printf("step 3\n");
			printf("step 2\n");
		}
	}
	printf("Done\n");
	return 0;
}


编程题
1
char ch;
int spaceCount = 0,returnCount = 0,otherCount = 0;

while((ch = getchar()) != '#') {
	if(ch == '\n') 
		returnCount++;
	else if(isspace(ch)) 
		spaceCount++;
	else
		otherCount++;
}
printf("space: %d, return %d, other :%d\n", spaceCount, returnCount, otherCount);

