1.
#include<stdio.h>
#define PER_MIN 60
int main(void) {
	int input, h, m;
	scanf("%d", &input);
	while(input > 0) {
		h = input / PER_MIN;
		m = input % PER_MIN;
		printf("%d hours and %d mins\n", h, m);
		scanf("%d", &input);
	}

	return 1;
}

2.
#include<stdio.h>
int main(void) {
	int i,e;
	scanf("%d", &i);
	e = i + 11;
	while(i++ < e) {
		printf("%d ", i-1);
	}
	printf("\n");
	return 1;
}

3.
#include<stdio.h>
int main()
{
	int input,w,d;
	scanf("%d",input);
	while(input > 0) {
		w = input / 7;
		d = input % 7;
		printf("%d days are %d weeks and %d days", %d, %d, %d);
		scanf("%d", input);
	}
	return 0;
}



5.
#include<stdio.h>
int main
{
	int const, sum, e;
	count = 0;
	sum = 0;
	scanf("%d", &e);
	while (count++ < e) {
		sum += count;
	}
	printf("sum = %d \n", sum);
	return 0;
}

7.
#include <stdio.h>
void _sqrt(double n);
int main(void){
	double n;
	scanf("%lf", &n);
	_sqrt(n);
	return 0;
}
void _sqrt(double n){
	double r = n * n * n;
	printf("%.4f", r);
}